<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>
<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <div class="modal-episode-type">
          <div class="modal-draw">
            <div class="modal-string"></div>
            <div class="modal-badge">
              <div class="modal-icon">
                <%= svg_tag #"babysteps-bottle.svg" %>
              </div>
            </div>
          </div>
          <h5 class="modal-title" id="exampleModalCenterTitle">Feeding</h5>
        </div>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <%= svg_tag #"modal-close.svg" %>
        </button>
      </div>

      <div class="modal-body modal-feeding">
        <div class="modal-time">
          <input type="text" placeholder="MAR/15/2019   2:32AM">
        </div>
        <div class="modal-food-type">
          <label for="">What has she eaten?</label>
          <input type="text" placeholder="Breast milk, banana, baby food...">
        </div>
        <div class="modal-note">
          <label for="">Would you like to mention something?</label>
          <input type="text" placeholder="Type it here">
        </div>
      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-episode btn-feeding"><span>Save</span></button>
      </div>
    </div>
  </div>
</div>

